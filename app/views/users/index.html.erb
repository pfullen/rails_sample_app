

<h1> Deacons</h1>


<table>
   <%= will_paginate %>
  <thead>
    <tr>
      <th>Last name</th>
      <th>First name</th>
      <th>Cell phone</th>
      <th>Email</th>
     
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr class="<%= cycle('even', 'odd') %>">
        <td><%= user.last_name %></td>
        <td><%= user.first_name %></td>
        <td><%= user.cell_phone %></td>
        <td><%= user.email %></td>
        
      </tr>
    <% end %>
      
  </tbody>

</table>

